

{{#each results}}

<div class = 'search-post'>
        <div class = 'search-post-subject'>{{this.subject}}</div>
        <div class = 'search-post-topic'>{{this.topic}}</div>
        <img class= 'search-post-image' src = {{this.imageurl}}>
        <div class = 'search-post-message'>{{this.content}}</div>
        <br>
        <div class = 'search-post-date'>{{this.date}}</div>
        <div class = 'search-post-reply'>{{this.replies}} replies</div>
    </div>

{{/each}}
